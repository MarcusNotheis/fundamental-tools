{
  "name": "rfmcall",
  "description": "ABAP RFM invocation template",
  "version": "1.3.1",
  "homepage": "https://github.com/sap/fundamental-tools",
  "author": {
    "name": "Srdjan Boskovic",
    "email": "srdjan.boskovic@sap.com"
  },
  "engines": {
    "node": "~10 >=10.23 || ~12 >=12.17 || >= 14.0"
  },
  "cpu": [
    "!arm"
  ],
  "directories": {
