  "devDependencies": {
    "@types/node": "^14.14.20",
    "@types/sprintf-js": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.17.0",
    "jest": "^26.6.3",
    "typescript": "^3.9.7"
  },
